//package Test; 
import java.util.Scanner; 
public class number_list { 
  
 
 
     public static void main(String[] args) { 
         Scanner scanner = new Scanner(System.in); 
         System.out.print("Enter the number of list: "); 
         int numberOfList = scanner.nextInt(); 
 
         int[] num = new int[numberOfList]; 
         System.out.println("Enter " + numberOfList + " numbers:"); 
 
         for (int i =  0; i < numberOfList; i++) { 
             num[i] = scanner.nextInt(); 
         } 
 
         System.out.println("The numbers you Enter are:"); 
         for (int number : num) { 
             System.out.println(number); 
         } 
 
         scanner.close(); 
     }  
 
}
